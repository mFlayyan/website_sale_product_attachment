<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--
            =========== Make min_date editable in picking form ===========
        -->
        <record
            id="ttr_view_picking_out_form_x1"
            model="ir.ui.view">
            <field name="name">ttr.stock.picking.out.form.x1</field>
            <field name="model">stock.picking</field>
            <field name="type">form</field>
            <field
                name="inherit_id"
                ref="stock.view_picking_out_form" />
            <field
                name="arch"
                type="xml">
                <data>
                    <field
                        name="min_date"
                        position="replace">
                        <field name="min_date" />
                    </field>
                </data>
            </field>
        </record>
    </data>
</openerp>